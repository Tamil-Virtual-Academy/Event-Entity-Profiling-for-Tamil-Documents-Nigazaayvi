            var ele;
            ele = $('#roadBlockAd');
            (typeof (ele) != "undefined" && ele.length>0) && ele.length > 0 && PutRoadBlockAd(ele.html().trim());
        });

        function PutRoadBlockAd(ad) {
            if (ad != undefined && ad != null && ad.trim() != '' && ad.replace(/>\s+</g, "><") != '<div></div>') {
                $('body').append('<style>#roadBlockAddModal img {width:100%;}</style>');
                $('body').append('<div id="roadBlockAddModal" class="modal fade " role="dialog" style="z-index:9999;"><div class="modal-dialog" style="width:800px;"><div><div style="position:absolute;right:0px;top: -20px;"><a style="color: #fff;font-size: 11px;cursor:pointer;" onclick="$(\'#roadBlockAddModal\').modal(\'hide\')">CLOSE X</a></div>' + ad + '</div></div></div>');
                $('#roadBlockAddModal').modal('show');
                $('#roadBlockAddModal').on('shown.bs.modal', function () {
                    setTimeout(function () {
                        $('#roadBlockAddModal').modal('hide');
                    }, 10000);
                });
            }
        }
    </script>
    <!--End RoadBolck-->
