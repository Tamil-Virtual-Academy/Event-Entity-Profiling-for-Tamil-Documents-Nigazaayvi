<script>
    $(document).ready(function () {
        var width = $(window).width();
        var ele;
        (width > 700) ? ele = $('#roadBlockAd-md') : ele = $('#roadBlockAd-xs');
        (typeof (ele) != "undefined") && PutRoadBlockAd(ele.html().trim());
    });

    function PutRoadBlockAd(ad) {
        if (ad != undefined && ad != null && ad.trim() != '' && ad.replace(/>\s+</g, "><") != '<div></div>') {
	    var width = $(window).width();
            var wid;
            (width > 700) ? wid = "800px" : wid = "100%";
            $('body').append('<style>#roadBlockAddModal img {width:100%;}</style>');
            $('body').append('<div id="roadBlockAddModal" class="modal fade " role="dialog"><div class="modal-dialog" style="width:'+ wid +'; top:20px;"><div><div style="position:absolute;right:0px;top: -20px;"><a style="color: #fff;font-size: 11px;cursor:pointer;" onclick="$(\'#roadBlockAddModal\').modal(\'hide\')">CLOSE X</a></div>' + ad + '</div></div></div>');
            $('#roadBlockAddModal').modal('show');
            $('#roadBlockAddModal').on('shown.bs.modal', function () {
                setTimeout(function () {
                    $('#roadBlockAddModal').modal('hide');
                }, 10000);
            });
        }
    }
    </script>

