    <script>
        $(document).ready(function () {
            if ($('#articleslider-container').html() == '') {
                GetNextArticle();
            }
        });
        var GetNextArticle = function () {
            $.ajax({
                type: 'POST',
                url: '/ArticleDetail/NextArticle',
                data: { articleId: 'ae025bec-65f1-4260-88bf-6add17b129a0', sectionId: '2' },
                success: function (data) {
                    $('#articleslider-container').html(data);
                    $(document).scroll(function () {
                        if (data != null) {
                            var distanceTop = $('#slidebararea').offset().top - $(window).height();
                            if ($(window).scrollTop() > distanceTop) {
                                if (document.getElementById('topover').style.display != 'block') {
                                    $('#slidebox').animate({ 'left': '0px' }, 1200);
                                }
                                else {
                                    document.getElementById('topover').style.visibility = 'visible';
                                }

                            } else {
                                document.getElementById('topover').style.visibility = 'hidden';
                                $('#slidebox').stop(true).animate({ 'left': '-530px' }, 700);
                            }
                        }
                    });
                    if (data != null) {
                        $('#slidebox .closee').bind('click', function () {
                            $('#slidebox').stop(true).animate({ 'left': '-530px' }, 10); document.getElementById('topover').style.display = 'block';
                            document.getElementById('topover').style.visibility = 'visible';
                            document.getElementById('topover').onclick = function () {
                                document.getElementById('topover').style.display = 'none';
                                $('#slidebox').animate({ 'left': '0px' }, 800);
                            }
                        });
                    }

                },
                error: function (result) {

                }
            });
        }
    </script>

<script>
        function share(s) {

            var url = window.location.href;

            if (s == 'f') {
                window.open('https://www.facebook.com/sharer.php?u=' + url + '&t=மாலை மலர் | செய்திகள் ' + 'sharer').focus();
            }
            else if (s == 't') {
                window.open('http://twitter.com/share?url=' + url, 'tweet').focus();
            }
            else if (s == 'g') {
                window.open('https://plus.google.com/share?url=' + url, 'sharer').focus();
            }
            else if (s == 'e') {
                window.location.href = "mailto:user@domain.com?subject=மாலை மலர் | செய்திகள் &body=" + url;

            }
            else if (s == 'p') {
                window.open('https://pinterest.com/pin/create%2Fbutton/?url=' + url + '&description=மாலை மலர் | செய்திகள் ').focus();
            }
        }

</script>
