                if ($("#fixTopMenuParent").offset().top < $(window).scrollTop()) {
                    var pad = parseInt($('.container').offset().left);
                    $("#fixTopMenu").css("position", 'fixed');
                    $("#fixTopMenu").css("z-index", '10002');
                    $("#fixTopMenu").css("top", '0');
                    $("#fixTopMenu").css("left", pad + 16);
