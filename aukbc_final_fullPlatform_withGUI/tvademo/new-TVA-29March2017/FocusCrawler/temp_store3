    function Headerlogin() {
        
            var win = window.open(_url, "windowname1", 'width=800, height=600');
            var pollTimer = window.setInterval(function () {
                try {
                    console.log(win.document.URL);
                    if (win.document.URL.indexOf(REDIRECT) != -1) {
                        window.clearInterval(pollTimer);
                        var url = win.document.URL;
                        
                        acToken = gup(url, 'access_token');
                        tokenType = gup(url, 'token_type');
                        expiresIn = gup(url, 'expires_in');
                        win.close();
                        validateTokenHeader(acToken);

                    }

                } catch (e) {
                }
            }, 500);

        }

        function validateTokenHeader(token) {
            $.ajax({
                url: VALIDURL + token,
                data: null,
                success: function (responseText) {
                    getUserInfoHeader();

                    loggedIn = true;
                    $('#loginText').hide();
                    $('#logoutText').show();
                },
                dataType: "jsonp"
            });
        }
        function getUserInfoHeader() {
            $.ajax({
                url: 'https://www.googleapis.com/oauth2/v1/userinfo?access_token=' + acToken,
                data: null,
                success: function (resp) {
                    user = resp;
                    console.log(user);
				if ((user.id != null) || (user.id == '')) {

                    //                    document.getElementById('fblogindiv').style.display = "none";
                    //                    document.getElementById('Twitterlogindivid').style.display = "none";
                    document.getElementById('HeaderGoogleLoginDiv').style.display = "none";
                    //alert(user.id);
                    var value = user.id; // +"&username=" + user.name + "&email=" + user.email;
                    var expiryDate = new Date();
                    expiryDate.setTime(expiryDate.setDate(expiryDate.getDate() + 1)); // 365 days
                    setCookie("readcookieuseridnew", value, expiryDate);


                    var value1 = user.name;
                    var expiryDate = new Date();
                    expiryDate.setTime(expiryDate.setDate(expiryDate.getDate() + 1)); // 365 days
                    setCookie("readcookieuserfullname", value1, expiryDate);


                    var value2 = user.email;
                    var expiryDate = new Date();
                    expiryDate.setTime(expiryDate.setDate(expiryDate.getDate() + 1)); // 365 days
                    setCookie("readcookieuseremailnew", value2, expiryDate);

                    var value3 = user.picture;
                    var expiryDate = new Date();
                    expiryDate.setTime(expiryDate.setDate(expiryDate.getDate() + 1)); // 365 days
                    setCookie("readcookieuserpicnew", value3, expiryDate);

                    //alert('You are succesfully logged in!');

                    if (glbfollowerstatus == "success") {
                        if (glbuserid != "") {
                            checknewuser("Gplus", "0", glbuserid);
                            //pagereloadfunRep();
                            
                        }
                    }
                    else if (headlikedisStatus == "success") {
                        checknewuser("Gplus","1","");
                        //pagereloadfunRep();
                        
                    }
                    else {
                        checknewuser("Gplus","2","");
                        //alert("thats right");
                        //pageloading();
                        
                    }
					}

                },
                dataType: "jsonp"
            });

        }

        function pageloading() {
            window.location.reload();
        }
    function clearListCookiesHead() {

        var cookiesRep = document.cookie.split(";");
        //window.location = "https://twitter.com/download?lang=en&logged_out=1";
        //alert(cookiesRep);
        alert("You have been successfully logged out");
        for (var i = 0; i < cookiesRep.length; i++) {
            var spcookRep = cookiesRep[i].split("=");
            //alert(spcookRep[0]);
            document.cookie = spcookRep[0] + "=;expires=Thu, 21 Sep 1979 00:00:01 UTC;path=/; domain=.dailythanthi.com;";
            document.cookie = spcookRep[0] + "=;expires=Thu, 21 Sep 1979 00:00:01 UTC;";
        }


        window.location.reload();
        Logout();
    }
    function delete_cookie(name) {
        document.cookie = name + '=; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
    }
    function setHomepage() {
        //Condition to set home page in IE
        
        var pathname = window.location.pathname;

        var CurrentURL = location.href;
       
        if (document.all) {
            document.body.style.behavior = 'url(#default#homepage)';
            document.body.setHomePage(CurrentURL);
        }
        //Condition to set home page in Mozilla
        else if (window.sidebar) {
            if (window.netscape) {
                try {
                    netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
                }
                catch (e) {
                    alert("this action was aviod by your browserï¼if you want to enableï¼please enter about:config in your address line,and change the value of signed.applets.codebase_principal_support to true");
                }
            }
            var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
            prefs.setCharPref('browser.startup.homepage', CurrentURL);
        }
    }

    function getCookieRep(name) {
        var valueRep = "; " + document.cookie;
        var partsRep = valueRep.split("; " + name + "=");
        if (partsRep.length == 2) return partsRep.pop().split(";").shift();
    }

    function FBHeaderLogin() {
            FB.login(function (response) {
                if (response.authResponse) {
                    var uid = "http://graph.facebook.com/" + response.authResponse.userID + "/picture";
                    var expiryDate = new Date();
                    expiryDate.setTime(expiryDate.setDate(expiryDate.getDate() + 1)); // 365 days
                    setCookie("readcookieuserpicnew", uid, expiryDate);
                    getFBHeaderUserInfo();
                } else {
                    console.log('User cancelled login or did not fully authorize.');
                }
            }, { scope: 'email,user_photos,user_videos,publish_stream' });
        }

        function getFBHeaderUserInfo() {
            FB.api('/me', function (response) {
                if ((response.id != null) || (response.id != "")) {
                var value = response.id; // +"&username=" + user.name + "&email=" + user.email;
                var expiryDate = new Date();
                expiryDate.setTime(expiryDate.setDate(expiryDate.getDate() + 1)); // 365 days
                setCookie("readcookieuseridnew", value, expiryDate);


                var value1 = response.name;
                var expiryDate = new Date();
                expiryDate.setTime(expiryDate.setDate(expiryDate.getDate() + 1)); // 365 days
                setCookie("readcookieuserfullname", value1, expiryDate);


                var value2 = response.email;
                var expiryDate = new Date();
                expiryDate.setTime(expiryDate.setDate(expiryDate.getDate() + 1)); // 365 days
                setCookie("readcookieuseremailnew", value2, expiryDate);

                if (glbfollowerstatus == "success") {
                    if (glbuserid != "") {
                        checknewuser("FaceBook", "0", glbuserid);
                    }
                }
                else if (headlikedisStatus == "success") {
                    checknewuser("FaceBook", "1", "");
                }
                else {
                    checknewuser("FaceBook", "2", "");
                }
				}


            });
        }

        function headerpopuplogin() {
            $find("mpe").show();
        }

