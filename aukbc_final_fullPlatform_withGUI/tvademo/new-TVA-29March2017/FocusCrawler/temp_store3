<script type="text/javascript">
    var M = jQuery.noConflict();
    M(window).bind("load", function () {
         var stickyNavTop = M('.MenuWholeDiv').offset().top;
        if (document.getElementById('menuId_submenu') != null) {
            var stickyNavTop1 = M('.subMenu_Css').offset().top;
        }
            var stickyNav = function () {
            var scrollTop = M(window).scrollTop(); 
            if (scrollTop > stickyNavTop) {
                     M('.MenuWholeDiv').addClass('sticky');
                  }
                    else {
                M('.MenuWholeDiv').removeClass('sticky');
              }
        };
        stickyNav();
      
        M(window).scroll(function () {
            stickyNav();
        });
    });

    
    </script>

    

