<script language="javascript" type="text/javascript">
    function HideAllMainMenu() {
        var selectId = arguments[0];
        var selectedCount = 0;
        var element;
        var MainMenuIds = getElementsByClassName("selectedHeaderMainMenuTab");
        var MainMenuId;
        for (var i = 0; i < MainMenuIds.length; i++) {
            MainMenuId = MainMenuIds[i].id;
            if (MainMenuId != selectId) {
                document.getElementById(MainMenuId).className = "unselectedHeaderMainMenuTab";
            }
        }
    }
    function chkMenuId() {
     //  alert('main'+arguments[0]);
       if (arguments[0] == arguments[1]) {
          // alert('main' + arguments[0]);
            HideAllMainMenu(arguments[0]);
            changeMainHeadBG(arguments[0]);
        }
    }
    function chkSubMenuId() {
       // alert(arguments[0]);
        if (arguments[0] == arguments[1]) {
            ChangeSubBG(arguments[0]);
        }
    }
</script>
