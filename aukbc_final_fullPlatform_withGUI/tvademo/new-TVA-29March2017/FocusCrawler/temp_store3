<script type="text/javascript">
    var F = jQuery.noConflict();
    F(document).ready(function() {
        var offset = 220;
        var duration = 500;
        F(window).scroll(function() {
        if (F(this).scrollTop() > offset) {
            F('.back-to-top').fadeIn(duration);
            } else {
            F('.back-to-top').fadeOut(duration);
            }
        });

        F('.back-to-top').click(function(event) {
            event.preventDefault();
            F('html, body').animate({ scrollTop: 0 }, duration);
            return false;
        })
		
		F('#yr').html((new Date()).getFullYear());
    });
	
</script>
